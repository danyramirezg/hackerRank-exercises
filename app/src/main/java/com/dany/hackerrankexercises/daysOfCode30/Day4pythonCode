class Person:
    def __init__(self,initialAge):
        self.initialAge = initialAge

    def amIOld(self):
        ageInstance = self.initialAge

        if(ageInstance < 0):
            print("Age is not valid, setting age to 0.")
            ageInstance = 0

        if (ageInstance < 13):
            print("You are young.")
        elif (ageInstance >=13 and ageInstance < 18):
            print("You are a teenager.")
        else:
            print("You are old.")

    def yearPasses(self):
        # Increment the age of the person in here
        self.initialAge += 1


t = int(input())
for i in range(0, t):
    age = int(input())
    p = Person(age)
    p.amIOld()
    for j in range(0, 3):
        p.yearPasses()
    p.amIOld()
    print("")